---
title: Nam Lemonade's Website
rowClass: justify-content-between
displayHeader: true
displayFooter: true
---


<!DOCTYPE html>
<html lang="en" class="theme-sea">

<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{{ title }}</title>
	<!-- Favicons -->
	<link rel="apple-touch-icon" sizes="57x57" href="{{ pathToGoTo }}favicon/apple-icon-57x57.png">
	<link rel="apple-touch-icon" sizes="60x60" href="{{ pathToGoTo }}favicon/apple-icon-60x60.png">
	<link rel="apple-touch-icon" sizes="72x72" href="{{ pathToGoTo }}favicon/apple-icon-72x72.png">
	<link rel="apple-touch-icon" sizes="76x76" href="{{ pathToGoTo }}favicon/apple-icon-76x76.png">
	<link rel="apple-touch-icon" sizes="114x114" href="{{ pathToGoTo }}favicon/apple-icon-114x114.png">
	<link rel="apple-touch-icon" sizes="120x120" href="{{ pathToGoTo }}favicon/apple-icon-120x120.png">
	<link rel="apple-touch-icon" sizes="144x144" href="{{ pathToGoTo }}favicon/apple-icon-144x144.png">
	<link rel="apple-touch-icon" sizes="152x152" href="{{ pathToGoTo }}favicon/apple-icon-152x152.png">
	<link rel="apple-touch-icon" sizes="180x180" href="{{ pathToGoTo }}favicon/apple-icon-180x180.png">
	<link rel="icon" type="image/png" sizes="192x192" href="{{ pathToGoTo }}favicon/android-icon-192x192.png">
	<link rel="icon" type="image/png" sizes="32x32" href="{{ pathToGoTo }}favicon/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="96x96" href="{{ pathToGoTo }}favicon/favicon-96x96.png">
	<link rel="icon" type="image/png" sizes="16x16" href="{{ pathToGoTo }}favicon/favicon-16x16.png">
	<link rel="manifest" href="{{ pathToGoTo }}favicon/manifest.json">
	<meta name="msapplication-TileColor" content="#2196f3">
	<meta name="msapplication-TileImage" content="{{ pathToGoTo }}favicon/ms-icon-144x144.png">
	<meta name="theme-color" content="#2196f3">
	<!-- Bootstrap -->
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet"
		integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"
		integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe"
		crossorigin="anonymous"></script>
	<link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
	<script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js" defer></script>
	<!-- Old Cursors -->
	<script src="https://unpkg.com/cursor-effects@latest/dist/browser.js"></script>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.11.5/css/lightbox.css" integrity="sha512-DKdRaC0QGJ/kjx0U0TtJNCamKnN4l+wsMdION3GG0WVK6hIoJ1UPHRHeXNiGsXdrmq19JJxgIubb/Z7Og2qJww==" crossorigin="anonymous" referrerpolicy="no-referrer" />
	<!-- Custom style sheet -->
	<link rel="stylesheet" href="https://furryring.neocities.org/onionring.css">
	<script src="https://kit.fontawesome.com/d5faa113aa.js" crossorigin="anonymous"></script>
	<link rel="stylesheet" type="text/css" href="{{ pathToGoTo }}icomoon/style.css">
	<link rel="stylesheet" href="{{ pathToGoTo }}styles/styles.css">
</head>

<body>
	{% if displayHeader == true %}
		<div class="title-website text-center">
			<div class="container">
				<div class="content d-flex align-items-end justify-content-end">
					<img class="land" src="{{ pathToGoTo }}images/animated-deer/yellow-wag.gif" alt="Deer">
					<img class="sea" src="{{ pathToGoTo }}images/animated-dolphin/dolphins-swim-together.gif" alt="Dolphins swimming">
					<div class="text">
						<h1>Nam Lemonade</h1>
						<h3 class="land">Welcome to the online pastures</h3>
						<h3 class="sea">Welcome to the cyber reef</h3>
					</div>
				</div>
			</div>
		</div>

		<div class="navbar">
			<div class="container">
				<div class="d-flex align-items-center w-100">
					<div class="home-icon" data-bs-toggle="tooltip" data-bs-title="Back home!"
						data-bs-custom-class="custom-tooltip">
						<a class="land" href="{{ pathToGoTo }}index.html">
							<img class="menu-default-image" src="{{ pathToGoTo }}images/polaroid-icon1.png" alt="Polaroid">
							<img class="menu-hover-image" src="{{ pathToGoTo }}images/polaroid-talk-closedeyes.gif"
								alt="Polaroid speaking gif">
						</a>
						<a class="sea" href="{{ pathToGoTo }}index.html">
							<img class="menu-default-image" src="{{ pathToGoTo }}images/reef-icon1.png" alt="Rose">
							<img class="menu-hover-image" src="{{ pathToGoTo }}images/reef-talk-closedeyes.gif"
								alt="Rose speaking gif">
						</a>

					</div>
					<div class="navbar-content w-100">
						<div class="navbar-header">
							<div class="d-flex align-items-center justify-content-center">
								<p>Menu</p>
							</div>
						</div>
						<div class="navbar-links">
							<div class="row align-items-center justify-content-center">
								<div class="col-auto text-center">
									<a href="{{ pathToGoTo }}about.html">About</a>
								</div>
								<div class="land col-auto text-center">
									<a href="{{ pathToGoTo }}polaroid.html">Who is that animal?</a>
								</div>
								<div class="sea col-auto text-center">
									<a href="{{ pathToGoTo }}rose.html">Who is that animal?</a>
								</div>
								{# <div class="col-auto text-center">
									<a href="{{ pathToGoTo }}art.html">The things I draw</a>
								</div> #}
								<div class="col-auto text-center">
									<a href="{{ pathToGoTo }}websites.html">Cool Webs</a>
								</div>
								<div class="col-auto text-center">
									<a href="{{ pathToGoTo }}flightrisingmodern.html">Flight Rising breeding tool</a>
								</div>
								<div class="col-auto text-center">
									<a href="{{ pathToGoTo }}blog.html">Blog</a>
								</div>
								<div class="col-auto text-center">
									<a href="{{ pathToGoTo }}wplace.html">WPlace stuff</a>
								</div>
								{# <div class="col-auto text-center">
									<a href="{{ pathToGoTo }}sitemap.html">Sitemap</a>
								</div> #}
								<div class="col-auto text-center">
									<a href="{{ pathToGoTo }}contact.html">Contact</a>
								</div>
								<div class="col-auto text-center">
									<a href="{{ pathToGoTo }}credits.html">Credits and special thanks</a>
								</div>
							</div>
						</div>
					</div>

				</div>
			</div>
		</div>
	{% endif %}


	<div class="content-page {{ contentPageClass }}">
		<div class="container">
			<div class="row {{ rowClass }}">
				{{ content | safe }}
			</div>
		</div>
	</div>


	{% if displayFooter == true %}
		<footer>
			<div class="footer-curve land">
				<svg id="y" viewBox="0 0 1057.78 59.6">
					<path class="b" style="fill:#4dac4d;"
						d="M1057.78,59.6H0C161.84,21.28,340.75,0,528.89,0s367.05,21.28,528.89,59.6Z"></path>
				</svg>
			</div>
			<div class="footer-curve sea">
				<svg id="y" viewBox="0 0 1057.78 59.6">
					<path class="b" style="fill:#b8e8fc;"
						d="M1057.78,59.6H0C161.84,21.28,340.75,0,528.89,0s367.05,21.28,528.89,59.6Z"></path>
				</svg>
				
			</div>
			<div class="footer-content">
				<div class="container">
					<div class="row">
						<div class="col-12">
							<div class="d-flex justify-content-center mb-3">
								<button type="button" class="button-switch-theme">
									<img class="sea" src="{{ pathToGoTo }}images/switch-theme-sea.png" alt="Switch themes">
									<img class="land" src="{{ pathToGoTo }}images/switch-theme-land.png" alt="Switch themes">
								</button>
							</div>
							<div class="footer-text">
								<p>Made with ❤️ by Nam "Rose" Lemonade. @2023</p>
							</div>
						</div>
					</div>
				</div>
			</div>
		</footer>
	{% endif %}

	<!-- script dot js -->
	<script src="https://code.jquery.com/jquery-3.7.1.min.js"
		integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.11.5/js/lightbox.min.js" integrity="sha512-KbRFbjA5bwNan6DvPl1ODUolvTTZ/vckssnFhka5cG80JVa5zSlRPCr055xSgU/q6oMIGhZWLhcbgIC0fyw3RQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
	<script>
		lightbox.option({
		'wrapAround': true,
		'disableScrolling': true,
		'alwaysShowNavOnTouchDevices': true
		})
	</script>
	{% if script %}
		<script src="{{ script }}"></script>
	{% endif %}
	<script src="{{ pathToGoTo }}js/scripts.js"></script>
</body>

</html>